

Code = "Kavya"
guess = " "
guessCount = 0
guessLimit = 3
out_of_guess = False

while Code != guess and not(out_of_guess):
    if guessCount < guessLimit:
        guess = input("Enter your guess: ")
        guessCount += 1
    else:
        out_of_guess = True


if guessCount > guessLimit:
    print("YOU LOSE!!")
else:
    print("YOU WIN")
